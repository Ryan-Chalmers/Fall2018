 {string} Y = {"y1", "y2", "y3", "y4","y5","y6","y7"};
 
 dvar int+ y[Y];
// dvar double z;

 
 minimize 100*y["y1"]-80*y["y2"]-25*y["y3"] - 20*y["y4"];
 
 subject to {
 	10*y["y1"] - y["y2"] >= 15;
 	15*y["y1"] - y["y2"] + 10*y["y5"] >= 25;
 	25*y["y1"] - y["y2"] - y["y5"] >= 40;
 	15*y["y1"] - y["y3"] >= 20;
 	25*y["y1"] - y["y3"] - 10*y["y6"] >= 40;
 	30*y["y1"] - y["y3"] - y["y6"] >= 50;
 	70*y["y1"] - y["y4"] >= 80;
 	100*y["y1"] - y["y4"] - 10*y["y7"] >= 110;
 	115*y["y1"] - y["y4"] - y["y7"] >= 130;
 	
 }
